<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CoinMapper">

	<insert id="bitInsert" parameterType="string">
       insert into bit(bit)
       values(#{bit})
    </insert>

	<select id="bitCount" resultType="int">
     select count(*)
     from bit
    </select>
    
 	<delete id="bitonedelete">
 	<![CDATA[
   	 delete from bit 
   	 where rownum <=1
   	]]> 
    </delete>   
    


</mapper>